{
  "name": "tree-sitter-menhir",
  "version": "0.5.0",
  "description": "Tree-sitter parser for Menhir files with OCaml language injection",
  "main": "bindings/node",
  "types": "bindings/node",
  "repository": "https://github.com/Kerl13/tree-sitter-menhir",
  "keywords": [
    "parser",
    "ocaml",
    "menhir",
    "tree-sitter",
    "incremental"
  ],
  "scripts": {
    "install": "node-gyp-build",
    "build": "tree-sitter generate",
    "test": "node --test bindings/node/*_test.js",
    "test:corpus": "tree-sitter test",
    "test:examples": "bash script/parse_examples.sh",
    "lint": "eslint grammar.js",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground"
  },
  "author": "Martin PÃ©pin",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "eslint-config-treesitter": "^1.0.2",
    "tree-sitter": "^0.25.0",
    "tree-sitter-cli": "^0.25.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "bindings/**",
    "queries/*",
    "src/**",
    "Cargo.toml"
  ]
}
